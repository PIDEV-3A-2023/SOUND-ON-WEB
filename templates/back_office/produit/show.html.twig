{% extends 'back_office/admin_dashboard/dashboard.html.twig' %}

{% block title %}Produit{% endblock %}

{% block content %}
<div class="pagetitle">
        <h1>Produit </h1>
        <nav>
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="index.html">Home</a></li>
                <li class="breadcrumb-item active">Dashboard</li>
            </ol>
        </nav>
    </div><!-- End Page Title -->
    <h1>Produit</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ produit.id }}</td>
            </tr>
            <tr>
                <th>Prix</th>
                <td>{{ produit.prix }}</td>
            </tr>
            <tr>
                <th>Image</th>
                <td>{{ produit.image }}</td>
            </tr>
            <tr>
                <th>Libelle</th>
                <td>{{ produit.libelle }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ produit.type }}</td>
            </tr>
            <tr>
                <th>Quantite</th>
                <td>{{ produit.quantite }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ produit.Description }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_back_office_produit_index') }}">back to list</a>

    <a href="{{ path('app_back_office_produit_edit', {'id': produit.id}) }}">edit</a>

    {{ include('back_office/produit/_delete_form.html.twig') }}
{% endblock %}
