{% extends 'base.html.twig' %}

{% block title %}Catalogue index{% endblock %}

{% block body %}
    <h1>Catalogue index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nom</th>
                <th>Image</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for catalogue in catalogues %}
            <tr>
                <td>{{ catalogue.id }}</td>
                <td>{{ catalogue.nom }}</td>
                <td>{{ catalogue.image }}</td>
                <td>
                 <a href="{{ path('app_back_office_catalogue_show', {'id': catalogue.id}) }}">show</a>
                    <a href="{{ path('app_back_office_catalogue_edit', {'id': catalogue.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table> 

   
    <a href="{{ path('app_back_office_catalogue_new') }}">Create new</a>
    

{% endblock %}
