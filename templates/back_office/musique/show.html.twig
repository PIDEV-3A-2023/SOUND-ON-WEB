{% extends 'base.html.twig' %}

{% block title %}Musique{% endblock %}

{% block body %}
    <h1>Musique</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ musique.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ musique.nom }}</td>
            </tr>
            <tr>
                <th>Chemin</th>
                <td>{{ musique.chemin }}</td>
            </tr>
            <tr>
                <th>DateCreation</th>
                <td>{{ musique.dateCreation ? musique.dateCreation|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Longueur</th>
                <td>{{ musique.longueur }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_back_office_musique_index') }}">back to list</a>

    <a href="{{ path('app_back_office_musique_edit', {'id': musique.id}) }}">edit</a>

    {{ include('back_office/musique/_delete_form.html.twig') }}
{% endblock %}
